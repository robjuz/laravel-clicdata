{
  "name": "robjuz/laravel-clickdata",
  "type": "library",
  "description": "ClicData integration for Laravel 5+",
  "keywords": [
    "podio",
    "laravel"
  ],
  "homepage": "https://github.com/robjuz/laravel-clicdata",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Juzak",
      "email": "robjuz@gmail.com",
      "homepage": "https://github.com/robjuz"
    }
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "laravel/framework": "~5.4",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "robjuz\\LaravelClickData\\": "src/robjuz/LaravelClickData/"
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },"extra": {
      "laravel": {
        "providers": [
          "robjuz\\LaravelClicData\\ServiceProvider"
        ],
        "aliases": {
          "ClicData": "robjuz\\LaravelClicData\\Facade"
        }
      }
    }
  }
}
